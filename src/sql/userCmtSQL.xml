<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="userCmt">

<select id="myGroupNo" parameterType="string" resultType="GroupMemberDTO">
	select * from GroupMember where id=#{id} and status=1
</select>

<select id="getMyGroupInfo" parameterType="int" resultType="GroupSpaceDTO">
	select * from GroupSpace where gNo=#{gNo} and status=4
</select>



</mapper>